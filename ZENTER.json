{
  "title": "ZEnter",
  "rules": [
    {
      "description": "Z + Enter = Escape",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "simultaneous": [
              {
                "key_code": "z"
              },
              {
                "key_code": "enter"
              }
            ]
            "modifiers": {
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "mission_control"
            }
          ]
        }
      ]
    }
  ]
}
